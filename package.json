{
  "name": "cleanstring-ts",
  "version": "1.1.4",
  "description": "Clean multiline TypeScript string literals",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "biome lint .",
    "format": "biome format . --write && prettier --write '*.md'",
    "format:md": "prettier --write '*.md'",
    "check": "biome check . && prettier --check '*.md'",
    "ci": "npm run lint && npm run check && npm run test",
    "prepublishOnly": "npm run build && npm run test && npm run check"
  },
  "keywords": [
    "string",
    "multiline",
    "clean",
    "typescript",
    "formatting"
  ],
  "author": "Arne Roomann-Kurrik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kurrik/cleanstring-ts.git"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
