    "ci": "npm run lint && npm run check && npm run test",
    "prepublishOnly": "npm run build && npm run test && npm run check"
  },
  "keywords": ["string", "multiline", "clean", "typescript", "formatting"],
  "author": "Arne Roomann-Kurrik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kurrik/cleanstring-ts.git"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3"
  }
}
