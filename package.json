{
  "name": "cleanstring-ts",
  "version": "1.0.0",
  "description": "Clean multiline TypeScript string literals",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/"],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "biome lint .",
    "format": "biome format . --write",
